{
    "name": "orchestra/testbench-browser-kit",
    "description": "Laravel Browser Kit Testing Helper for Packages Development",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Testbench\\BrowserKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Testbench\\BrowserKit\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/browser-kit-testing": "^6.3",
        "orchestra/testbench": "^7.6.1",
        "symfony/dom-crawler": "^5.4 || ^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.x-dev"
        }
    },
    "scripts": {
        "prepare": [
            "@composer run post-autoload-dump"
        ],
        "test": [
            "@composer run prepare",
            "@php vendor/bin/phpunit -c ./ --color"
        ],
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "require-dev": {
        "laravel/pint": "^1.1"
    }
}
