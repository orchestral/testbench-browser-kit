{
    "name": "orchestra/testbench-browser-kit",
    "description": "Laravel Browser Kit Testing Helper for Packages Development",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Testbench\\BrowserKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Testbench\\BrowserKit\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/browser-kit-testing": "^6.3",
        "orchestra/testbench": "^7.36",
        "symfony/dom-crawler": "^6.0.9"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "phpstan/phpstan": "^1.10.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.x-dev"
        }
    },
    "scripts": {
        "post-autoload-dump": "@prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "lint": [
            "@php vendor/bin/phpstan analyse",
            "@php vendor/bin/pint"
        ],
        "test": "@php vendor/bin/phpunit -c ./ --color",
        "ci": [
            "@prepare",
            "@lint",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
