{
    "name": "orchestra/testbench-browser-kit",
    "description": "Laravel Browser Kit Testing Helper for Packages Development",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Testbench\\BrowserKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Testbench\\BrowserKit\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/browser-kit-testing": "^6.4 || ^7.0",
        "orchestra/testbench": "^8.0",
        "symfony/dom-crawler": "^6.2"
    },
    "require-dev": {
        "laravel/pint": "^1.4",
        "phpstan/phpstan": "^1.9.14"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "ci": [
            "@composer run prepare",
            "@php vendor/bin/phpstan analyse",
            "@php vendor/bin/pint",
            "@composer run test"
        ],
        "test": "@php vendor/bin/phpunit -c ./ --color"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
